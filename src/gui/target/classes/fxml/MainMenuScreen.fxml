<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="695.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <Label text="Management objednávek">
               <VBox.margin>
                  <Insets bottom="10.0" left="20.0" top="10.0" />
               </VBox.margin>
            </Label>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#handleNewOrderInButtonAction" prefHeight="100.0" prefWidth="100.0" text="Nová logistická objednavka" textAlignment="CENTER" wrapText="true">
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                  </Button>
                  <Button disable="true" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Nová koncová objednavka" textAlignment="CENTER" wrapText="true">
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                  </Button>
                  <Button disable="true" layoutX="110.0" layoutY="10.0" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Změna logistické objednávky" textAlignment="CENTER" wrapText="true">
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                  </Button>
                  <Button disable="true" layoutX="240.0" layoutY="10.0" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Změna koncové objednávky" textAlignment="CENTER" wrapText="true">
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <Label text="Správa zboží">
               <VBox.margin>
                  <Insets bottom="10.0" left="20.0" top="10.0" />
               </VBox.margin>
            </Label>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button disable="true" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Naskladnění" textAlignment="CENTER" wrapText="true">
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                  </Button>
                  <Button disable="true" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Předáni k expedici" textAlignment="CENTER" wrapText="true">
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                  </Button>
                  <Button disable="true" layoutX="75.0" layoutY="10.0" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Přesun zbozi" textAlignment="CENTER" wrapText="true">
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <Label layoutX="30.0" layoutY="290.0" text="Zobrazování údajů v systému">
               <VBox.margin>
                  <Insets bottom="10.0" left="20.0" top="10.0" />
               </VBox.margin>
            </Label>
            <HBox layoutX="10.0" layoutY="180.0" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button disable="true" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Reporty" textAlignment="CENTER" wrapText="true">
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <Label layoutX="30.0" layoutY="155.0" text="Administrace">
               <VBox.margin>
                  <Insets bottom="10.0" left="20.0" top="10.0" />
               </VBox.margin>
            </Label>
            <HBox layoutX="10.0" layoutY="180.0" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#handleAddProductTypeButtonAction" prefHeight="100.0" prefWidth="100.0" text="Nový typ zboži" textAlignment="CENTER" wrapText="true">
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#handleChangeProductTypeButtonAction" prefHeight="100.0" prefWidth="100.0" text="Úprava typu zboží" textAlignment="CENTER" wrapText="true">
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                  </Button>
                  <Button disable="true" layoutX="75.0" layoutY="10.0" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Nalezení pohřešovaného zboží" textAlignment="CENTER" wrapText="true">
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                  </Button>
                  <Button disable="true" layoutX="240.0" layoutY="10.0" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Nový uživatel" textAlignment="CENTER" wrapText="true">
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                  </Button>
                  <Button disable="true" layoutX="340.0" layoutY="10.0" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Úprava uživatele" textAlignment="CENTER" wrapText="true">
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                  </Button>
                  <Button disable="true" layoutX="440.0" layoutY="10.0" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Blokace uživatele" textAlignment="CENTER" wrapText="true">
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </center>
   <bottom>
      <VBox>
         <children>
            <Separator />
            <HBox fx:id="sessionBar" BorderPane.alignment="CENTER" />
         </children>
      </VBox>
   </bottom>
</BorderPane>
