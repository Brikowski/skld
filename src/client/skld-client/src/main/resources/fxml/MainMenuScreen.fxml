                        <Insets left="15.0" />
                     </HBox.margin>
                  </Button>
                  <Button disable="true" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Uprava typu zbozi" textAlignment="CENTER" wrapText="true">
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                  </Button>
                  <Button disable="true" layoutX="75.0" layoutY="10.0" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Nalezeni pohresovaneho zbozi" textAlignment="CENTER" wrapText="true">
