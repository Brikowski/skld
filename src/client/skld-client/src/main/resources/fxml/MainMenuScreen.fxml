                  <Button disable="true" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Nova logisticka objednavka" textAlignment="CENTER" wrapText="true">
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                  </Button>
                  <Button disable="true" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Nova koncova objednavka" textAlignment="CENTER" wrapText="true">
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                  </Button>
                  <Button disable="true" layoutX="110.0" layoutY="10.0" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Zmena logisticke objednavky" textAlignment="CENTER" wrapText="true">
