                  <Insets bottom="10.0" left="20.0" top="10.0" />
               </VBox.margin>
            </Label>
            <HBox layoutX="10.0" layoutY="180.0" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#handleAddProductTypeButtonAction" prefHeight="100.0" prefWidth="100.0" text="Novy typ zbozi" textAlignment="CENTER" wrapText="true">
